<h1>Listing concursos</h1>

<table>
  <thead>
    <tr>
      <th>Nombre</th>
      <th>Banner</th>
      <th>Url</th>
      <th>Fechaini</th>
      <th>Fechafin</th>
      <th>Premio</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @concursos.each do |concurso| %>
      <tr>
        <td><%= concurso.nombre %></td>
        <td><%= concurso.banner %></td>
        <td><%= concurso.url %></td>
        <td><%= concurso.fechaini %></td>
        <td><%= concurso.fechafin %></td>
        <td><%= concurso.premio %></td>
        <td><%= link_to 'Show', concurso %></td>
        <td><%= link_to 'Edit', edit_concurso_path(concurso) %></td>
        <td><%= link_to 'Destroy', concurso, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Concurso', new_concurso_path %>
