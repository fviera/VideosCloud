<div class="container">
<h1>Lista administradores</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Correo</th>
      <th>Empresa</th>
      <th>Usuario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @administradors.each do |administrador| %>
      <tr>
        <td><%= administrador.nombres %></td>
        <td><%= administrador.apellidos %></td>
        <td><%= administrador.correo %></td>
        <td><%= administrador.empresa %></td>
        <td><%= administrador.usuario %></td>
        <td width="40"><%= link_to 'Ver', administrador %></td>
        <td width="50"><%= link_to 'Editar', edit_administrador_path(administrador) %></td>
        <td width="50"><%= link_to 'Eliminar', administrador, method: :delete, data: { confirm: 'Esta seguro?' } %></td>    </tr>
    <% end %>
  </tbody>
</table>
  <%= will_paginate @administrador, renderer: BootstrapPagination::Rails %>
</div>
<br>

<%= link_to 'Nuevo Administrador', new_administrador_path, class: 'btn btn-primary'%>
