<div class="container">
<h2>Listado de videos</h2>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Nombres</th>
      <th>Apellidos</th>
      <th>Correo</th>
      <th>Fecha</th>
      <th>Estado</th>
      <th>Archivo</th>
      <th>Archivo Convertido</th>
      <th>Fecha inicio Con.</th>
      <th>Fecha fin Con.</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @videos.each do |video| %>
      <tr>
        <td><%= video.nombres %></td>
        <td><%= video.apellidos %></td>
        <td><%= video.correo %></td>
        <td><%= video.fecha %></td>
        <td><%= video.estado %></td>
        <td><%= video.archivo %></td>
        <td><%= video.archivoconvertido %></td>
        <td><%= video.fechainiconversion %></td>
        <td><%= video.fechafinconversion %></td>
        <td><%= link_to 'Ver', video %></td>
        <td><%= link_to 'Editar', edit_video_path(video) %></td>
        <td><%= link_to 'Eliminar', video, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>
  <%= will_paginate @video, renderer: BootstrapPagination::Rails %>
</div>
<br>

<%= link_to 'New Video', new_video_path %>
